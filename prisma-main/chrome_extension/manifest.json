{
    "manifest_version": 3,
    "name": "Prisma",
    "version": "1.0",
    "permissions": [
      "scripting",
      "tabs",
      "windows",
      "activeTab",
      "downloads"
    ],
    "host_permissions": [
      "https://www.linkedin.com/analytics/creator/content/"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
}